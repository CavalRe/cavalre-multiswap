{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "run-p build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "node -r dotenv/config node_modules/.bin/remix dev",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@mantine/core": "^4.0.0",
    "@mantine/dates": "^4.0.0",
    "@mantine/dropzone": "^4.0.0",
    "@mantine/hooks": "^4.0.0",
    "@mantine/modals": "^4.0.0",
    "@mantine/next": "^4.0.0",
    "@mantine/notifications": "^4.0.0",
    "@mantine/prism": "^4.0.0",
    "@mantine/rte": "^4.0.0",
    "@mantine/spotlight": "^4.0.0",
    "@mantine/ssr": "^3.6.14",
    "@remix-run/react": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@types/marked": "^4.0.2",
    "@web3auth/base": "^0.3.1",
    "@web3auth/web3auth": "^0.3.3",
    "dayjs": "^1.10.8",
    "front-matter": "^4.0.2",
    "marked": "^4.0.12",
    "moralis": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-moralis": "^1.3.2",
    "remix": "^1.2.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "dotenv": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
